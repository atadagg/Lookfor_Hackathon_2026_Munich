# OpenAI API Key (required)
OPENAI_API_KEY=your-openai-api-key-here

# Shopify API URL (hackathon tooling endpoint)
# Leave empty for local/mock mode
# Set to hackathon API URL when provided
API_URL=

# Backend URL for test scripts
# Default: http://localhost:8000
# Set when testing against deployed backend (e.g., https://your-app.railway.app)
BACKEND_URL=http://localhost:8000

# LangSmith (optional - for tracing)
LANGSMITH_TRACING=false
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=hackathon

# MinIO Storage Configuration (UC2: Wrong Item photo uploads)
# Hackathon organizers will provide these credentials
MINIO_URL=
MINIO_ENDPOINT=storage.aimentora.com
MINIO_BUCKET=gangbucket
MINIO_ACCESS_KEY=
MINIO_SECRET_KEY=
MINIO_USE_SSL=true
